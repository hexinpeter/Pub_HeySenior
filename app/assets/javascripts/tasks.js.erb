$(document).ready(function() {
  taskTableClickable();
});

function taskTableClickable() {
  $('#task-table tr').click( function() {
      window.location = $(this).find('a').attr('href');
  }).hover( function() {
      $(this).toggleClass('hover');
  });
}

function grayOutPage() {
  $("[id$='-page']").addClass("gray-out-page");
}
